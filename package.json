{
  "name": "express-parse-query-sequelize",
  "version": "0.0.3",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/queryParserMiddleware.d.js",
  "scripts": {
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint": "npx eslint ./src --ext .ts,.json",
    "build": "ts-node build.ts",
    "start:dev": "npx nodemon",
    "package": "npm run build && npm pack",
    "publish": "npm run build && npm link && npm publish --access public",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "addscope": "node tools/packagejson name express-parse-query-sequelize"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "express",
    "sequelize",
    "parser",
    "query",
    "params",
    "typescript",
    "querystring",
    "querystring-parser",
    "graph"
  ],
  "author": "Wesle Smitthe <wesleysmitthe22@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wesleysmitthe/express-parse-query-sequelize",
  "repository": {
    "type": "git",
    "url": "git@github.com:wesleysmitthe/express-parse-query-sequelize.git"
  },
  "bugs": {
    "url": "https://github.com/wesleysmitthe/express-parse-query-sequelize/issues"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "npx ts-node --files -r ./src",
    "ext": "ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.1",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "nodemon": "^2.0.15",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "sequelize": "^6.12.5",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
